<%= render :partial => "user/accounts/account_menu" %>
<h2>Create New Job</h2>
<%= simple_form_for(@job, :url => user_account_company_jobs_path(@user, @company), :html => { :class => "form-horizontal" }) do |f| %>
  
  <%= render :partial => 'form', :locals => {:f => f} %>
  
  <div class="form-actions">
    <%= f.submit "Create", :class => "btn btn-primary"%>
    <%= link_to "Back", :back, :class => "btn btn-info" %>
  </div>
<% end %>
